        android:minSdkVersion="15"
        android:targetSdkVersion="24" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.dionimfxgmail.avaliacao.LoginActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.dionimfxgmail.avaliacao.MainActivity" />
        <activity
            android:name="com.dionimfxgmail.avaliacao.Calculadora"
            android:parentActivityName="com.dionimfxgmail.avaliacao.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name="com.dionimfxgmail.avaliacao.Cadastro_Livro"
            android:parentActivityName="com.dionimfxgmail.avaliacao.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
/>
        <activity
            android:name="com.dionimfxgmail.avaliacao.Consulta_Livro"
            android:parentActivityName="com.dionimfxgmail.avaliacao.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
/>
    </application>

</manifest>